{
  "name": "mmirror",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "npm-run-all -p dev:*",
    "dev:app": "nodemon --watch server.js server.js",
    "dev:apollo-codegen": "yarn apollo-codegen --watch",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "apollo-codegen": "yarn apollo codegen:generate --addTypename --queries '{pages,components,components/**/!(__generated__)}/*.js' --schema ../server/src/schema.graphqls --target flow --useFlowReadOnlyTypes"
  },
  "devDependencies": {
    "@babel/preset-flow": "^7.0.0",
    "apollo": "^1.9.2",
    "babel-eslint": "^9.0.0",
    "babel-plugin-graphql-tag": "^1.6.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.81.0",
    "flow-typed": "^2.5.1",
    "googleapis": "^33.0.0",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.2"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.2.10",
    "apollo-client": "^2.4.2",
    "apollo-link": "^1.2.3",
    "apollo-link-error": "^1.1.1",
    "apollo-link-http": "^1.5.5",
    "apollo-link-ws": "^1.0.9",
    "apollo-utilities": "^1.0.21",
    "fast-memoize": "^2.5.1",
    "graphql": "0.13.2",
    "graphql-tag": "^2.9.2",
    "js-cookie": "^2.2.0",
    "koa": "^2.5.3",
    "koa-compress": "^3.0.0",
    "koa-router": "^7.4.0",
    "next": "^7.0.0-canary.19",
    "next-routes": "^1.4.2",
    "node-fetch": "^2.2.0",
    "pino": "^5.6.1",
    "qrcode.react": "^0.8.0",
    "react": "^16.5.1",
    "react-apollo": "^2.1.11",
    "react-dom": "^16.5.1",
    "subscriptions-transport-ws": "^0.9.15",
    "uuid": "^3.3.2"
  }
}
